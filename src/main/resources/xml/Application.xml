<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.ApplicationDao">

    <select id="selectapplication" resultType="entity.Application">
        SELECT * FROM `application`
    </select>
    <update id="updateappcation">
      UPDATE application SET application_numer=#{application_numer} WHERE application_id=#{application_id}
    </update>

</mapper>